{
  "name": "this-is-us",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "postcss ./assets/css/townhall.css -o ./static/css/townhall.built.css",
    "watch:css": "postcss ./assets/css/townhall.css -o ./static/css/townhall.built.css --watch",
    "build:hugo": "hugo --gc --minify",
    "build": "npm run build:css && npm run build:hugo",
    "dev:hugo": "hugo server",
    "dev": "npm-run-all --parallel watch:css dev:hugo"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "dotenv": "^16.5.0",
    "firebase-admin": "^13.4.0",
    "itty-router": "^5.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
